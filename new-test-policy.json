{
  "configuration": {
    "custom_metrics": {
      "auth": {
        "credential_type": ""
      },
      "metric_submission_strategy": {
        "allow_from": "bound_app"
      }
    }
  },
  "instance_min_count": 1,
  "instance_max_count": 2,
  "scaling_rules": [
    {
      "metric_type": "test_metric",
      "breach_duration_secs": 60,
      "threshold": 500,
      "operator": ">=",
      "cool_down_secs": 60,
      "adjustment": "+1"
    }
  ]
}
