{
  "packages": {
    "path:.#bosh-bootloader":            "",
    "path:.#app-autoscaler-cli-plugin":  "",
    "path:.#log-cache-cli-plugin":       "",
    "path:.#cloud-mta-build-tool":       "",
    "path:.#uaac":                       "",
    "actionlint":                        "1.7.1",
    "bosh-cli":                          "7.3.1",
    "bundix":                            "latest",
    "cloudfoundry-cli":                  "8.7.11",
    "coreutils":                         "latest",
    "delve":                             "latest",
    "direnv":                            "2.34.0",
    "fly":                               "7.10.0",
    "gh":                                "latest",
    "ginkgo":                            "2.20.0",
    "gnumake":                           "4.4",
    "gnutar":                            "latest",
    "go-tools":                          "latest",
    "gopls":                             "latest",
    "gum":                               "0.13.0",
    "jq":                                "latest",
    "maven":                             "3.8.6",
    "nodejs":                            "latest",
    "nodePackages.yaml-language-server": "latest",
    "oha":                               "latest",
    "pre-commit":                        "latest",
    "postgresql":                        "latest",
    "python":                            "latest",
    "rubocop":                           "latest",
    "rubyPackages.solargraph":           "latest",
    "shellcheck":                        "0.10.0",
    "swagger-cli":                       "latest",
    "which":                             "latest",
    "yq-go":                             "4.44.2",
    "go":                                "1.22.2",
    "golangci-lint":                     "1.60.2",
    "credhub-cli":                       "2.9.29",
    "markdownlint-cli2":                 "latest",
    "act":                               "0.2.63",
    "google-cloud-sdk":                  "latest",
    "temurin-bin-21":                    "latest",
    "ruby":                              "latest"
  },
  "shell": {
    "init_hook": [
      "install_plugin_if_needed() {",
      "  sum=$(sha1sum $(which $1) | cut --delimiter=' ' --fields='1')",
      "  cf plugins --checksum | grep --quiet ${sum} || cf install-plugin -f $(which $1)",
      "}",
      "install_plugin_if_needed 'app-autoscaler-cli-plugin'",
      "install_plugin_if_needed 'log-cache-cli'"
    ]
  }
}
